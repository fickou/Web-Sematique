<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.ugb.sn/ressources/ens-sup.owl#"
     xml:base="http://www.ugb.sn/ressources/ens-sup.owl"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:foaf="http://xmlns.com/foaf/0.1/"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:ens="http://www.ugb.sn/ressources/ens-sup.rdfs#">

  <owl:Ontology rdf:about="http://www.ugb.sn/ressources/ens-sup.owl"/>

  <!-- ===================== -->
  <!-- CLASSES PRINCIPALES -->
  <!-- ===================== -->

  <!-- Ressources Humaines -->
  <owl:Class rdf:about="ens:Personne">
    <rdfs:label xml:lang="fr">Personne</rdfs:label>
    <rdfs:comment xml:lang="fr">Représente une personne impliquée dans l'enseignement supérieur</rdfs:comment>
    <rdfs:subClassOf rdf:resource="foaf:Person"/>
  </owl:Class>

  <owl:Class rdf:about="ens:Enseignant">
    <rdfs:label xml:lang="fr">Enseignant</rdfs:label>
    <rdfs:comment xml:lang="fr">Personne qui enseigne dans un établissement d'enseignement supérieur</rdfs:comment>
    <rdfs:subClassOf rdf:resource="ens:Personne"/>
    <owl:disjointWith rdf:resource="ens:Etudiant"/>
    <owl:disjointWith rdf:resource="ens:PersonnelAdministratif"/>
  </owl:Class>

  <owl:Class rdf:about="ens:Chercheur">
    <rdfs:label xml:lang="fr">Chercheur</rdfs:label>
    <rdfs:comment xml:lang="fr">Personne qui effectue des travaux de recherche</rdfs:comment>
    <rdfs:subClassOf rdf:resource="ens:Personne"/>
  </owl:Class>

  <owl:Class rdf:about="ens:EnseignantChercheur">
    <rdfs:label xml:lang="fr">Enseignant-Chercheur</rdfs:label>
    <rdfs:comment xml:lang="fr">Personne qui cumule les fonctions d'enseignement et de recherche</rdfs:comment>
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <rdf:Description rdf:about="ens:Enseignant"/>
          <owl:Restriction>
            <owl:onProperty rdf:resource="ens:membreDe"/>
            <owl:someValuesFrom rdf:resource="ens:Laboratoire"/>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
  </owl:Class>

  <owl:Class rdf:about="ens:Etudiant">
    <rdfs:label xml:lang="fr">Étudiant</rdfs:label>
    <rdfs:comment xml:lang="fr">Personne inscrite dans une formation de l'enseignement supérieur</rdfs:comment>
    <rdfs:subClassOf rdf:resource="ens:Personne"/>
    <owl:disjointWith rdf:resource="ens:Enseignant"/>
    <owl:disjointWith rdf:resource="ens:PersonnelAdministratif"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="ens:inscritDans"/>
        <owl:minCardinality rdf:datatype="xsd:nonNegativeInteger">1</owl:minCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="ens:Doctorant">
    <rdfs:label xml:lang="fr">Doctorant</rdfs:label>
    <rdfs:comment xml:lang="fr">Étudiant inscrit en doctorat</rdfs:comment>
    <rdfs:subClassOf rdf:resource="ens:Etudiant"/>
  </owl:Class>

  <owl:Class rdf:about="ens:PersonnelAdministratif">
    <rdfs:label xml:lang="fr">Personnel Administratif</rdfs:label>
    <rdfs:comment xml:lang="fr">Personnel non enseignant travaillant dans l'administration</rdfs:comment>
    <rdfs:subClassOf rdf:resource="ens:Personne"/>
    <owl:disjointWith rdf:resource="ens:Enseignant"/>
    <owl:disjointWith rdf:resource="ens:Etudiant"/>
  </owl:Class>

  <!-- Structures Académiques -->
  <owl:Class rdf:about="ens:Universite">
    <rdfs:label xml:lang="fr">Université</rdfs:label>
    <rdfs:comment xml:lang="fr">Établissement d'enseignement supérieur et de recherche</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="ens:Faculte">
    <rdfs:label xml:lang="fr">Faculté / UFR</rdfs:label>
    <rdfs:comment xml:lang="fr">Unité de Formation et de Recherche au sein d'une université</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="ens:EcoleDoctorale">
    <rdfs:label xml:lang="fr">École Doctorale</rdfs:label>
    <rdfs:comment xml:lang="fr">Structure d'accueil et de formation des doctorants</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="ens:Departement">
    <rdfs:label xml:lang="fr">Département</rdfs:label>
    <rdfs:comment xml:lang="fr">Division administrative au sein d'une faculté</rdfs:comment>
  </owl:Class>

  <!-- Formations -->
  <owl:Class rdf:about="ens:Formation">
    <rdfs:label xml:lang="fr">Formation</rdfs:label>
    <rdfs:comment xml:lang="fr">Programme d'études proposé par un établissement</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="ens:contientUE"/>
        <owl:minCardinality rdf:datatype="xsd:nonNegativeInteger">1</owl:minCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="ens:Licence">
    <rdfs:label xml:lang="fr">Licence</rdfs:label>
    <rdfs:comment xml:lang="fr">Formation de niveau Bac+3</rdfs:comment>
    <rdfs:subClassOf rdf:resource="ens:Formation"/>
  </owl:Class>

  <owl:Class rdf:about="ens:Master">
    <rdfs:label xml:lang="fr">Master</rdfs:label>
    <rdfs:comment xml:lang="fr">Formation de niveau Bac+5</rdfs:comment>
    <rdfs:subClassOf rdf:resource="ens:Formation"/>
  </owl:Class>

  <owl:Class rdf:about="ens:Doctorat">
    <rdfs:label xml:lang="fr">Doctorat</rdfs:label>
    <rdfs:comment xml:lang="fr">Formation de niveau Bac+8 préparant au grade de docteur</rdfs:comment>
    <rdfs:subClassOf rdf:resource="ens:Formation"/>
  </owl:Class>

  <owl:Class rdf:about="ens:UniteEnseignement">
    <rdfs:label xml:lang="fr">Unité d'Enseignement</rdfs:label>
    <rdfs:comment xml:lang="fr">Module composant une formation</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="ens:estEnseigneePar"/>
        <owl:minCardinality rdf:datatype="xsd:nonNegativeInteger">1</owl:minCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="ens:Maquette">
    <rdfs:label xml:lang="fr">Maquette de Formation</rdfs:label>
    <rdfs:comment xml:lang="fr">Description structurée du contenu d'une formation</rdfs:comment>
  </owl:Class>

  <!-- Recherche -->
  <owl:Class rdf:about="ens:Laboratoire">
    <rdfs:label xml:lang="fr">Laboratoire</rdfs:label>
    <rdfs:comment xml:lang="fr">Structure de recherche regroupant des chercheurs</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="ens:ProjetRecherche">
    <rdfs:label xml:lang="fr">Projet de Recherche</rdfs:label>
    <rdfs:comment xml:lang="fr">Initiative de recherche avec objectifs et financement</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="ens:Publication">
    <rdfs:label xml:lang="fr">Publication Scientifique</rdfs:label>
    <rdfs:comment xml:lang="fr">Document publié présentant des résultats de recherche</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="ens:ResultatRecherche">
    <rdfs:label xml:lang="fr">Résultat de Recherche</rdfs:label>
    <rdfs:comment xml:lang="fr">Produit issu d'un travail de recherche</rdfs:comment>
  </owl:Class>

  <!-- Ressources Matérielles -->
  <owl:Class rdf:about="ens:RessourceMaterielle">
    <rdfs:label xml:lang="fr">Ressource Matérielle</rdfs:label>
    <rdfs:comment xml:lang="fr">Équipement ou infrastructure physique</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="ens:Imprimante3D">
    <rdfs:label xml:lang="fr">Imprimante 3D</rdfs:label>
    <rdfs:subClassOf rdf:resource="ens:RessourceMaterielle"/>
  </owl:Class>

  <owl:Class rdf:about="ens:SuperCalculateur">
    <rdfs:label xml:lang="fr">Super Calculateur</rdfs:label>
    <rdfs:subClassOf rdf:resource="ens:RessourceMaterielle"/>
  </owl:Class>

  <owl:Class rdf:about="ens:ServeurCloud">
    <rdfs:label xml:lang="fr">Serveur Cloud</rdfs:label>
    <rdfs:subClassOf rdf:resource="ens:RessourceMaterielle"/>
  </owl:Class>

  <owl:Class rdf:about="ens:Incubateur">
    <rdfs:label xml:lang="fr">Incubateur</rdfs:label>
    <rdfs:comment xml:lang="fr">Structure d'accompagnement de startups et projets innovants</rdfs:comment>
    <rdfs:subClassOf rdf:resource="ens:RessourceMaterielle"/>
  </owl:Class>

  <!-- Infrastructures -->
  <owl:Class rdf:about="ens:Infrastructure">
    <rdfs:label xml:lang="fr">Infrastructure</rdfs:label>
    <rdfs:comment xml:lang="fr">Bâtiment ou espace au sein de l'université</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="ens:Batiment">
    <rdfs:label xml:lang="fr">Bâtiment</rdfs:label>
    <rdfs:subClassOf rdf:resource="ens:Infrastructure"/>
  </owl:Class>

  <owl:Class rdf:about="ens:SalleClasse">
    <rdfs:label xml:lang="fr">Salle de Classe</rdfs:label>
    <rdfs:subClassOf rdf:resource="ens:Infrastructure"/>
  </owl:Class>

  <owl:Class rdf:about="ens:Amphitheatre">
    <rdfs:label xml:lang="fr">Amphithéâtre</rdfs:label>
    <rdfs:subClassOf rdf:resource="ens:Infrastructure"/>
  </owl:Class>

  <owl:Class rdf:about="ens:Bureau">
    <rdfs:label xml:lang="fr">Bureau</rdfs:label>
    <rdfs:subClassOf rdf:resource="ens:Infrastructure"/>
  </owl:Class>

  <owl:Class rdf:about="ens:Restaurant">
    <rdfs:label xml:lang="fr">Restaurant</rdfs:label>
    <rdfs:subClassOf rdf:resource="ens:Infrastructure"/>
  </owl:Class>

  <owl:Class rdf:about="ens:InstallationSportiveCulturelle">
    <rdfs:label xml:lang="fr">Installation Sportive et Culturelle</rdfs:label>
    <rdfs:subClassOf rdf:resource="ens:Infrastructure"/>
  </owl:Class>

  <!-- ===================== -->
  <!-- PROPRIÉTÉS -->
  <!-- ===================== -->

  <owl:ObjectProperty rdf:about="ens:enseigne">
    <rdfs:label xml:lang="fr">enseigne</rdfs:label>
    <rdfs:comment xml:lang="fr">Relie un enseignant à une unité d'enseignement</rdfs:comment>
    <rdfs:domain rdf:resource="ens:Enseignant"/>
    <rdfs:range rdf:resource="ens:UniteEnseignement"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="ens:estEnseigneePar">
    <rdfs:label xml:lang="fr">est enseignée par</rdfs:label>
    <rdfs:comment xml:lang="fr">Relie une UE à son enseignant</rdfs:comment>
    <rdfs:domain rdf:resource="ens:UniteEnseignement"/>
    <rdfs:range rdf:resource="ens:Enseignant"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="ens:inscritDans">
    <rdfs:label xml:lang="fr">inscrit dans</rdfs:label>
    <rdfs:comment xml:lang="fr">Relie un étudiant à sa formation</rdfs:comment>
    <rdfs:domain rdf:resource="ens:Etudiant"/>
    <rdfs:range rdf:resource="ens:Formation"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="ens:inscritDansED">
    <rdfs:label xml:lang="fr">inscrit dans école doctorale</rdfs:label>
    <rdfs:comment xml:lang="fr">Relie un doctorant à son école doctorale</rdfs:comment>
    <rdfs:domain rdf:resource="ens:Doctorant"/>
    <rdfs:range rdf:resource="ens:EcoleDoctorale"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="ens:appartientA">
    <rdfs:label xml:lang="fr">appartient à</rdfs:label>
    <rdfs:comment xml:lang="fr">Relie un département à sa faculté</rdfs:comment>
    <rdfs:domain rdf:resource="ens:Departement"/>
    <rdfs:range rdf:resource="ens:Faculte"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="ens:offre">
    <rdfs:label xml:lang="fr">offre</rdfs:label>
    <rdfs:comment xml:lang="fr">Relie une faculté à une formation proposée</rdfs:comment>
    <rdfs:domain rdf:resource="ens:Faculte"/>
    <rdfs:range rdf:resource="ens:Formation"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="ens:contientUE">
    <rdfs:label xml:lang="fr">contient UE</rdfs:label>
    <rdfs:comment xml:lang="fr">Relie une formation à ses unités d'enseignement</rdfs:comment>
    <rdfs:domain rdf:resource="ens:Formation"/>
    <rdfs:range rdf:resource="ens:UniteEnseignement"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="ens:faitPartieDe">
    <rdfs:label xml:lang="fr">fait partie de</rdfs:label>
    <rdfs:comment xml:lang="fr">Relie une faculté à son université</rdfs:comment>
    <rdfs:domain rdf:resource="ens:Faculte"/>
    <rdfs:range rdf:resource="ens:Universite"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="ens:membreDe">
    <rdfs:label xml:lang="fr">membre de</rdfs:label>
    <rdfs:comment xml:lang="fr">Relie un chercheur à son laboratoire</rdfs:comment>
    <rdfs:domain rdf:resource="ens:Chercheur"/>
    <rdfs:range rdf:resource="ens:Laboratoire"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="ens:participeA">
    <rdfs:label xml:lang="fr">participe à</rdfs:label>
    <rdfs:comment xml:lang="fr">Relie un chercheur à un projet de recherche</rdfs:comment>
    <rdfs:domain rdf:resource="ens:Chercheur"/>
    <rdfs:range rdf:resource="ens:ProjetRecherche"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="ens:publie">
    <rdfs:label xml:lang="fr">publie</rdfs:label>
    <rdfs:comment xml:lang="fr">Relie un chercheur à une publication</rdfs:comment>
    <rdfs:domain rdf:resource="ens:Chercheur"/>
    <rdfs:range rdf:resource="ens:Publication"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="ens:dirigePar">
    <rdfs:label xml:lang="fr">dirigé par</rdfs:label>
    <rdfs:comment xml:lang="fr">Relie un doctorant à son directeur de thèse</rdfs:comment>
    <rdfs:domain rdf:resource="ens:Doctorant"/>
    <rdfs:range rdf:resource="ens:EnseignantChercheur"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="ens:rattacheA">
    <rdfs:label xml:lang="fr">rattaché à</rdfs:label>
    <rdfs:comment xml:lang="fr">Relie un laboratoire à une université</rdfs:comment>
    <rdfs:domain rdf:resource="ens:Laboratoire"/>
    <rdfs:range rdf:resource="ens:Universite"/>
  </owl:ObjectProperty>

  <!-- Propriétés Métadonnées -->
  <owl:ObjectProperty rdf:about="ens:nom">
    <rdfs:label xml:lang="fr">nom</rdfs:label>
    <rdfs:comment xml:lang="fr">Nom d'une ressource</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="foaf:name"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="ens:email">
    <rdfs:label xml:lang="fr">email</rdfs:label>
    <rdfs:comment xml:lang="fr">Adresse email</rdfs:comment>
    <rdfs:domain rdf:resource="ens:Personne"/>
    <rdfs:subPropertyOf rdf:resource="foaf:mbox"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="ens:titre">
    <rdfs:label xml:lang="fr">titre</rdfs:label>
    <rdfs:comment xml:lang="fr">Titre d'une publication ou d'une formation</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="dc:title"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="ens:dateCreation">
    <rdfs:label xml:lang="fr">date de création</rdfs:label>
    <rdfs:comment xml:lang="fr">Date de création de la ressource</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="dc:date"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="ens:credits">
    <rdfs:label xml:lang="fr">crédits ECTS</rdfs:label>
    <rdfs:comment xml:lang="fr">Nombre de crédits ECTS pour une UE ou formation</rdfs:comment>
    <rdfs:domain rdf:resource="ens:UniteEnseignement"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="ens:capacite">
    <rdfs:label xml:lang="fr">capacité</rdfs:label>
    <rdfs:comment xml:lang="fr">Capacité d'accueil d'une infrastructure</rdfs:comment>
    <rdfs:domain rdf:resource="ens:Infrastructure"/>
  </owl:ObjectProperty>

</rdf:RDF>
